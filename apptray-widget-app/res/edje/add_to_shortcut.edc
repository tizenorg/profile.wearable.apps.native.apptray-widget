					align : 0.5 0.5;
					image {
						normal: "apps_edit_icon_select_ef.png";
					}
					color_class: "AO0341";
					visible: 1;
				}
			}
		} /* end of parts */
		programs {
			program{
				name: "mouse_down";
				signal: "mouse,down,*";
				source: "icon";
				action: SIGNAL_EMIT "mouse_down" "*";
			}
			program{
				name: "mouse_up";
				signal: "mouse,up,*";
				source: "icon";
				action: SIGNAL_EMIT "mouse_up" "*";
			}
			program{
				name: "mouse_clicked";
				signal: "mouse,clicked,*";
				source: "icon";
				action: SIGNAL_EMIT "mouse_clicked" "*";
			}
			program {
				signal: "selected";
				source: "item";
				action: STATE_SET "show" 0.0;
				target: "icon_select";
				target: "icon_select_line";
			}
			program {
				signal: "normal";
				source: "item";
				action: STATE_SET "default" 0.0;
				target: "icon_select";
				target: "icon_select_line";
			}
			program {
				signal: "focus_in_effect";
				source: "item";
				action: STATE_SET "default" 0.0;
				target: "icon_clipper";
				transition : CUBIC_BEZIER 0.4 0.25 0.46 0.45 1.0;
			}
			program {
				signal: "focus_out_effect";
				source: "item";
				action: STATE_SET "focus_out" 0.0;
				target: "icon_clipper";
				transition : CUBIC_BEZIER 0.3 0.25 0.46 0.45 1.0;
			}
		}
	} /* end of item group */
