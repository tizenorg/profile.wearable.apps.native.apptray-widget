collections {
	base_scale: 1.3;
	group {
		name: "layout";
		parts {
			part {
				name: "bg";
				type: RECT;
				mouse_events: 1;
//				clip_to: "bg,clip";
				description {
					state: "default" 0.0;
					rel1 { relative: 0.0 0.0; offset: 0 0; }
					rel2 { relative: 1.0 1.0; offset: 0 0; }
					//image { normal: "apps_widget_preview_bg_dark.png"; }
					visible: 0;
				}
				description {
					state: "dark_theme" 0.0;
					rel1 { relative: 0.0 0.0; offset: 0 0; }
					rel2 { relative: 1.0 1.0; offset: 0 0; }
					//image { normal: "apps_widget_preview_bg_dark.png"; }
					color: 0 0 0 255;
					visible: 1;
				}/*
				description {
					state: "light_theme" 0.0;
					rel1 { relative: 0.0 0.0; offset: 0 0; }
					rel2 { relative: 1.0 1.0; offset: 0 0; }
					image { normal: "apps_widget_preview_bg_light.png"; }
					visible: 1;
				}*/
			}
			part {
				name: "scroller";
				type: SWALLOW;
				mouse_events: 1;
				description {
					state: "default" 0.0;
					rel1 { relative: 0.0 0.0; to : "bg"; }
					rel2 { relative: 1.0 1.0; to : "bg"; }
					align: 0.5 0.0;
					visible: 1;
				}
			}
			part {
				name: "title";
				type: TEXTBLOCK;
				mouse_events : 0;
				description {
					state: "default" 0.0;
					rel1 {
						relative: 0.5 27/360;
						to : "bg";
					}
					rel2 {
						relative: 0.5 (27+39)/360;
						to : "bg";
					}
					color_class: "AT015";
					align: 0.5 0.5;
					min : 234 39;
					max : 234 39;
					text {
						style: "title_style";
						align: 0.5 0.5;
					}
					visible : 0;
				}
			}
			part {
				name: "name";
				type: TEXTBLOCK;
				mouse_events : 0;
				description {
					state: "default" 0.0;
					rel1 {
						relative: 0.5 258/360;
						to : "bg";
					}
					rel2 {
						relative: 0.5 (258+39)/360;
						to : "bg";
					}
					color_class: "AT016";
					align: 0.5 0.5;
					min : 234 40;
					max : 234 40;
					text {
						style: "name_style";
						align: 0.5 0.5;
					}
					visible : 0;
				}
				description {
					state: "show" 0.0;
					rel1 {
						relative: 0.5 258/360;
						to : "bg";
					}
					rel2 {
						relative: 0.5 (258+39)/360;
						to : "bg";
					}
					color_class: "AT016";
					align: 0.5 0.5;
					min : 234 40;
					max : 234 40;
					visible : 1;
				}
			}
			part {
				name: "index_region";
				type: SPACER;
				scale: 1;
				mouse_events: 1;
				description {
					state: "default" 0.0;
					rel1 { relative: 0.0 0.0; to: "bg"; }
					rel2 { relative: 1.0 1.0; to: "bg"; }
				}
			}
			part {
				name: "index";
				type: SWALLOW;
				mouse_events: 1;
				description {
					state: "default" 0.0;
					rel1 { relative: 0.0 0.0; to: "index_region"; }
					rel2 { relative: 1.0 1.0; to: "index_region"; }
					visible: 1;
				}
			}
		} /* end of parts */
		programs {
			program{
				name : "show_name";
				signal : "show_name";
				source : "name";
				action : STATE_SET "show" 0.0;
				target : "name";
			}
			program{
				name : "hide_name";
				signal : "hide_name";
				source : "name";
				action : STATE_SET "default" 0.0;
				target : "name";
			}
			program {
				signal: "dark_theme";
				source: "bg";
				action: STATE_SET "dark_theme" 0.0;
				target: "bg";
			}
			/*
			program {
				signal: "light_theme";
				source: "bg";
				action: STATE_SET "light_theme" 0.0;
				target: "bg";
			}
			*/
		}
	} /* end of layout group */
	group {
		name: "item";
		parts {
			part {
				name: "bg";
				type: SWALLOW;
				mouse_events: 1;
				description {
