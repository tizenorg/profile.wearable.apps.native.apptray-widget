					min : 100 100;
					max : 100 100;
					align : 0.5 0.5;
					image {
						normal: "apps_edit_icon_bg_ef.png";
					}
					visible: 1;
				}
			}/*
			part {
				name: "icon";
				type: SWALLOW;
				clip_to: "icon_clipper";
				mouse_events: 1;
				repeat_events : 1;
				description {
					state: "default" 0.0;
					rel1 {
						relative: 0.5 0.5;
						to : "bg";
					}
					rel2 {
						relative: 0.5 0.5;
						to : "bg";
					}
					min : 100 100;
					max : 100 100;
					align : 0.5 0.5;
					visible: 1;
				}
			}*/
			part {
				name: "icon_select_line";
				type: IMAGE;
				mouse_events: 1;
				repeat_events : 1;
				description {
					state: "default" 0.0;
					rel1 { relative: 0.0 0.0; to: "icon"; offset: -1 -1;}
					rel2 { relative: 1.0 1.0; to: "icon"; offset: -1 -1;}
					min : 142 142;
					max : 142 142;
					align : 0.5 0.5;
					image {
						normal: "apps_edit_icon_select_ef.png";
					}
					color_class: "AO0341";
					visible: 0;
				}
				description {
					state: "show" 0.0;
					rel1 { relative: 0.0 0.0; to: "icon"; offset: -1 -1;}
					rel2 { relative: 1.0 1.0; to: "icon"; offset: -1 -1;}
