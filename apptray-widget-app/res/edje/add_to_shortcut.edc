					image {
						normal: "apps_edit_icon_bg_ef.png";
					}
					visible: 1;
				}
				description {
					state: "focus_out" 0.0;
					rel1 { relative: 0.0 0.0; to: "icon"; }
					rel2 { relative: 1.0 1.0; to: "icon"; }
