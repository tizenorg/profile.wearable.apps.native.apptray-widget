	group {
		name: "edit_layout";
		parts {
			part {
				name: "bg";
				type: RECT;
				mouse_events: 1;
//				clip_to: "bg,clip";
				description {
					state: "default" 0.0;
					rel1 { relative: 0.0 0.0; offset: 0 0; }
					rel2 { relative: 1.0 1.0; offset: 0 0; }
					//image { normal: "apps_widget_preview_bg_dark.png"; }
					visible: 0;
				}
				description {
					state: "dark_theme" 0.0;
					rel1 { relative: 0.0 0.0; offset: 0 0; }
					rel2 { relative: 1.0 1.0; offset: 0 0; }
					//image { normal: "apps_widget_preview_bg_dark.png"; }
					color : 0 0 0 255;
					visible: 1;
				}
