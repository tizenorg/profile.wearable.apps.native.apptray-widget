			part {
				name: "index1";
				type: SWALLOW;
				mouse_events: 1;
				description {
					state: "default" 0.0;
					rel1 {
						relative:  130/WIDGET_W 21/WIDGET_H;
						to : "bg";
					}
					rel2 {
						relative: (130+SHORTCUT_W)/WIDGET_W (21+SHORTCUT_H)/WIDGET_H;
						to : "bg";
					}
					color : 0 0 0 0;
					visible: 1;
				}
			}
			part {
				name: "index2";
				type: SWALLOW;
				mouse_events: 1;
				description {
